class NamedCow extends Cow
{
  private String myName;    
  private String namedType = Cow.getType();
  public NamedCow(String type, String name, String sound)    
  {       
    Cow.getType() = type;                                          
    myName = name; 
    Cow.getSound() = sound;
  }     
  public NamedCow()   
  {         
    myName = "unknown";   
  }
  String getName()
  {
  	return myName;
  }  
}